<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="nanumTextView" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/custom_font_family</item>
    </style>

    <style name="nanumEditTextView" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/custom_font_family</item>
    </style>

    <style name="nanumButton" parent="Widget.AppCompat.Button">
        <item name="fontFamily">@font/custom_font_family</item>
    </style>

    <style name="saveButton" parent="android:Widget.Button">
        <item name="android:background">@color/SaveButtonBack</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">30sp</item>
        <item name="android:layout_width">169dp</item>
        <item name="android:layout_height">84dp</item>
    </style>

    <style name="cancelButton" parent="android:Widget.Button">
        <item name="android:background">@color/CancelButtonBack</item>
        <item name="android:textColor">@color/CancelButtonText</item>
        <item name="android:textSize">30sp</item>
        <item name="android:layout_width">169dp</item>
        <item name="android:layout_height">84dp</item>
    </style>

    <style name="id_settingButton" parent="android:Widget.Button">
        <item name="android:background">@color/SaveButtonBack</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">500dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="colorControlHighlight">#0000ff</item>
    </style>
</resources>